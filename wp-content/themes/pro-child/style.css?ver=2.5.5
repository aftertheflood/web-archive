/* Sparks classes 
----------------------------------------------------------------------------- */

.sparks-standfirst         {font-family: Sparks-Bar-Medium;         letter-spacing: 0em;}

.sparks-bar-extranarrow    {font-family: Sparks-Bar-Extranarrow;    letter-spacing: 0em;}
.sparks-bar-narrow         {font-family: Sparks-Bar-Narrow;         letter-spacing: 0em;}
.sparks-bar-medium         {font-family: Sparks-Bar-Medium;         letter-spacing: 0em;}
.sparks-bar-wide           {font-family: Sparks-Bar-Wide;           letter-spacing: 0em;}
.sparks-bar-extrawide      {font-family: Sparks-Bar-Extrawide;      letter-spacing: 0em;}

.sparks-dot-extrasmall     {font-family: Sparks-Dot-Extrasmall;     letter-spacing: 0em;}
.sparks-dot-small          {font-family: Sparks-Dot-Small;          letter-spacing: 0em;}
.sparks-dot-medium         {font-family: Sparks-Dot-Medium;         letter-spacing: 0em;}
.sparks-dot-large          {font-family: Sparks-Dot-Large;          letter-spacing: 0em;}
.sparks-dot-extralarge     {font-family: Sparks-Dot-Extralarge;     letter-spacing: 0em;}

.sparks-dotline-extrathin  {font-family: Sparks-Dotline-Extrathin;  letter-spacing: 0em;}
.sparks-dotline-thin       {font-family: Sparks-Dotline-Thin;       letter-spacing: 0em;}
.sparks-dotline-medium     {font-family: Sparks-Dotline-Medium;     letter-spacing: 0em;}
.sparks-dotline-thick      {font-family: Sparks-Dotline-Thick;      letter-spacing: 0em;}
.sparks-dotline-extrathick {font-family: Sparks-Dotline-Extrathick; letter-spacing: 0em;}

.sparks-standfirst,
.sparks-bar-extranarrow,
.sparks-bar-narrow,
.sparks-bar-medium,
.sparks-bar-wide,
.sparks-bar-extrawide,
.sparks-dot-extrasmall,
.sparks-dot-small,
.sparks-dot-medium,
.sparks-dot-large,
.sparks-dot-extralarge,
.sparks-dotline-extrathin,
.sparks-dotline-thin,
.sparks-dotline-medium,
.sparks-dotline-thick,
.sparks-dotline-extrathick {
  font-variant-ligatures: contextual;
  -moz-font-feature-settings: "calt";
  -webkit-font-feature-settings: "calt";
  font-feature-settings: "calt";
  font-size: 115%;
}